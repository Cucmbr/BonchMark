<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    x:Class="BonchMarkBlazor.MainPage"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:BonchMarkBlazor"
    xmlns:pages="clr-namespace:BonchMarkBlazor.Pages"
    xmlns:xwpages="clr-namespace:BonchMarkBlazor.XamlPages.WeekPages"
    NavigationPage.HasBackButton="False"
    NavigationPage.HasNavigationBar="False">

    <BlazorWebView HostPage="wwwroot/index.html">
        <BlazorWebView.RootComponents>
            <RootComponent ComponentType="{x:Type local:Main}" Selector="#app" />
        </BlazorWebView.RootComponents>
    </BlazorWebView>

</ContentPage>